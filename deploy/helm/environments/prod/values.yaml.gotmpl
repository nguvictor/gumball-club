ingress:
  enabled: true
  hosts:
    - host: {{ .StateValues.ci.ingressDomain }}
      paths:
        - path: /
          pathType: Prefix

alertmanager:
  env: "prod"
  nameOverride: "gumball-club-amcfg"
  slackConfig:
    channel: "feed--alerts-dapps-prod"
  secrets:
    region: eu-west-2
    name: "rtlj-prod/eks/dapps/dev/alertmanager-integration-keys"
